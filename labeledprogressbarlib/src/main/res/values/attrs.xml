<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GProgress">
        <attr name="progressBeginColor" format="color" />
        <attr name="progressCompleteColor" format="color" />
        <attr name="labelValueType" format="enum">
            <enum name="percentage" value="0" />
            <enum name="value" value="1" />
            <enum name="custom" value="2" />
        </attr>
        <attr name="labelBackgroundColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="indeterminate" format="boolean" />
        <attr name="progressHeight" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="value" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="minValue" format="integer" />
        <attr name="labelText" format="string" />
    </declare-styleable>
</resources>